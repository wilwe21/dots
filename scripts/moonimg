#!/bin/bash
path=$1
x=$(curl wttr.in/?format=%m)
if [ $x == ðŸŒ‘ ]; then
  echo "8quart.png"
  exit
elif [ $x == ðŸŒ• ]; then
  echo "4quart.png"
  exit
elif [ $x == ðŸŒ’ ]; then
  echo "7quart.png"
  exit
elif [ $x == ðŸŒ˜ ]; then 
  echo "1quart.png"
  exit
elif [ $x == ðŸŒ“ ]; then
  echo "1quart.png"
  exit
elif [ $x == ðŸŒ— ]; then
  echo "2quart.png"
  exit
elif [ $x == ðŸŒ” ]; then
  echo "5quart.png"
  exit
elif [ $x == ðŸŒ– ]; then
  echo "3quart.png"
  exit
else
  echo "null.png"
  exit
fi
